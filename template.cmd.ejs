

<% if (repositories.length > 1) { %>
Repositories:

<% repositories.forEach(rep => { %>
    <%= rep %>
<% }) %>

<% } %>
<% if (config.graph == 'short'||config.graph == 'all') { %>
Short statistic
<% _(authors).forEach(author => { %>
<%= minSize(author.name) %> [<%= author.graphLine %>] <%= (author.percent * config.barSize).toFixed(2) %>%
<% }) %>
/Short statistic
<% } %>

<% if (config.graph == 'detailed' || config.graph =='all') { %>
Detailed statistic
<% _(authors).forEach(author => { %>
<%= minSize(author.name) %> [<%= author.graphLine %>] <%= (author.percent * config.barSize).toFixed(2) %>%
<% author.byExt.forEach((ext, index) => { %>
<%=  minSize(`${index < author.byExt.length -1 ? '├── ' : '└── '}${ext.name}`) %> [<%= ext.graphLine %>] <%= (ext.percent * config.barSize).toFixed(2) %>% <%= ext.extensions.join(', ')%>
<% }) %>
<% }) %>
/Detailed statistic
<% } %>

<%= table %>